    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^5.4.1",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
