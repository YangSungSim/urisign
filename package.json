    "test:e2e:result": "npm run test:e2e -- --verbose --noStackTrace 2>&1 | grep -v '‚óè' | grep -ve '[Ee]xpected' | grep -v 'npm' | grep -v 'Received' | grep -ve '^$'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^7.4.0",
    "csrf": "^3.1.0",
